<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th>Projectname</th>
      <th>Projectimage</th>
      <th>Projectdescription</th>
      <th>Projectclient</th>
      <th>Projectcollaborators</th>
      <th>Projectstart</th>
      <th>Projectend</th>
      <th>Projectlink</th>
      <th>Projecttag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.projectName %></td>
        <td><%= project.projectImage %></td>
        <td><%= project.projectDescription %></td>
        <td><%= project.projectClient %></td>
        <td><%= project.projectCollaborators %></td>
        <td><%= project.projectStart %></td>
        <td><%= project.projectEnd %></td>
        <td><%= project.projectLink %></td>
        <td><%= project.projectTag %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
